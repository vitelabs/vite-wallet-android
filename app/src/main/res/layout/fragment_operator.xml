<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="730dp"
        android:background="#F6F6F7"
        tools:context="net.vite.wallet.exchange.OperatorFragment">


    <ImageView
            android:id="@+id/operatorIcon"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="11dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@mipmap/bugcoin_buyin_icon" />

    <TextView
            android:id="@+id/operatorName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:textColor="#24272B"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/operatorIcon"
            app:layout_constraintStart_toEndOf="@+id/operatorIcon"
            app:layout_constraintTop_toTopOf="@+id/operatorIcon"
            tools:text="Vite Labs" />

    <View
            android:id="@+id/view114"
            android:layout_width="0dp"
            android:layout_height="0.5dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="24dp"
            android:background="#D3DFEF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/operatorIcon" />

    <TextView
            android:id="@+id/operatorAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="@string/operator_address"
            android:textColor="#993E4A59"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/operatorIcon"
            app:layout_constraintTop_toBottomOf="@+id/view114" />

    <TextView
            android:id="@+id/operatorAddressTxt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="24dp"
            android:textColor="#3E4A59"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/operatorIcon"
            app:layout_constraintTop_toBottomOf="@+id/operatorAddress"
            tools:text="vite_4c2c19f563187163145ab8f53f5bd36864756996e47a7vite_4c2c19f563187163145ab8f53f5bd36864756996e47a7vite_4c2c19f563187163145ab8f53f5bd36864756996e47a7" />

    <TextView
            android:id="@+id/operatorIntoTxt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="24dp"
            android:textColor="#3E4A59"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/operatorIcon"
            app:layout_constraintTop_toBottomOf="@+id/textView38"
            tools:text="vite_4c2c19f563187163145ab8f53f5bd36864756996e47a7" />

    <View
            android:id="@+id/view14"
            android:layout_width="0dp"
            android:layout_height="0.5dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="24dp"
            android:background="#D3DFEF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/operatorAddressTxt" />

    <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:text="@string/intro"
            android:textColor="#993E4A59"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/operatorAddress"
            app:layout_constraintTop_toBottomOf="@+id/view14" />

    <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="@string/trade_pair"
            android:textColor="#993E4A59"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/operatorAddress"
            app:layout_constraintTop_toBottomOf="@+id/operatorIntoTxt" />

    <TextView
            android:id="@+id/tradePair"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:lineSpacingExtra="6dp"
            android:textColor="#007AFF"
            app:layout_constraintStart_toStartOf="@+id/textView39"
            app:layout_constraintTop_toBottomOf="@+id/textView39"
            tools:text="VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX VITE_VX " />
</androidx.constraintlayout.widget.ConstraintLayout>
